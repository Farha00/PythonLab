      "source": [
        "n=int(input('Enter a 4-digit number = '))\n",
        "dig1=n%10\n",
        "dig2=(n//10)%10\n",
        "dig3=(n//100)%10\n",
        "dig4=(n//1000)%10\n",
        "sum=dig1+dig2+dig3+dig4\n",
        "print('Sum : ',sum)\n",
        "print('Reverse : ',(dig1*1000)+(dig2*100)+(dig3*10)+dig4)\n",
        "print((dig4*dig2)-(dig3*dig1))"
      ]
